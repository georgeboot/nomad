{
    "name": "dyrynda/nomad",
    "description": "Laravel-style database migrations wherever they may roam",
    "type": "library",
    "homepage": "https://github.com/michaeldyrynda/nomad",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Dyrynda",
            "email": "michael@dyrynda.com.au"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/cache": "5.5.*",
        "illuminate/config": "5.5.*",
        "illuminate/console": "5.5.*",
        "illuminate/container": "5.5.*",
        "illuminate/database": "5.5.*",
        "illuminate/events": "5.5.*",
        "illuminate/filesystem": "5.5.*",
        "laravel-zero/foundation": "5.5.*",
        "symfony/process": "^4.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Dyrynda\\Nomad\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "bin": ["nomad"],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
